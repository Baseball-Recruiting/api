{
    "name": "baseball-recruiting/api",
    "description": "API package for Recruit.Me",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "Recruit.Me",
        "Baseball Recruiting",
        "recruit.me api"
    ],
    "homepage": "https://recruit.me",
    "authors": [
        {
            "name": "Recruit.Me",
            "email": "petenystrom@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "RecruitMe\\Api\\": "src"
        }
    },
    "require": {
        "knuckleswtf/scribe": "^4.22"
    },
    "require-dev": {
        "laravel/pint": "^1.5.0",
        "phpunit/phpunit": "^9.0|^10.0|^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "RecruitMe\\Api\\Providers\\ApiServiceProvider"
            ],
            "aliases": {
                "ApiHelper": "RecruitMe\\Api\\Facades\\ApiHelper"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
